<div class="">
  <h1>Create an Idea Box Account</h1>
</div>
<div class="">
  <%= form_for @user do |f|  %>
    <%=  f.label :name %>
    <%= f.text_field :name %>

    <%=  f.label :email %>
    <%= f.text_field :email %>

    <%=  f.label :password %>
    <%= f.password_field :password %>

    <%=  f.label :password_confirmation, 'Confirm Password' %>
    <%= f.password_field :password_confirmation %>

    <%= f.submit %>

  <% end %>
</div>
