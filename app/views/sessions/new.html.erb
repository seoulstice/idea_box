<div class="">
  <section id="sesion-new-form">
    <div>
      <h1>.ideaBox LOG IN</h1>
    </div>
    <div>
      <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.text_field :password %>

      <%= f.submit "Log In"%>
      <% end %>
    </div>
    <div>
      Need an account? <%= link_to "Create Account", new_user_path %>
    </div>
  </section>
  <section>
    <div id="plant">

    </div>
  </section>
</div>
